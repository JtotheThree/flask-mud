{
  "name": "flask-mud",
  "version": "1.0.0",
  "description": "A poorly named PnP mobile website.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -n \"FUSE,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"node fuse.js\" \"npm run flask-server\"",
