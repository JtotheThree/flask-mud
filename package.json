  "bugs": {
    "url": "https://github.com/JtotheThree/flask-mud/issues"
  },
  "homepage": "https://github.com/JtotheThree/flask-mud#readme",
  "devDependencies": {
    "@types/socket.io-client": "latest",
    "bootstrap": "latest",
    "concurrently": "^3.5.1",
    "daemonite-material": "latest",
    "fuse-box": "latest",
    "node-sass": "latest",
    "jquery": "^3.3.1",
    "socket.io-client": "^2.0.4",
    "typescript": "latest"
  }
}
